true => dom(a), dom(b), dom(c).
true => tREFLEXIVE_REWRITE(a,b).
true => tREFLEXIVE_REWRITE(a,c).

tREFLEXIVE_REWRITE(V1,V2),
fREFLEXIVE_REWRITE(V1,V2) => false.
tEQUALISH(V1,V2), fEQUALISH(V1,V2) => false.
tREWRITE(V1,V2), fREWRITE(V1,V2) => false.

tREFLEXIVE_REWRITE(b,A), tREFLEXIVE_REWRITE(c,A) => false.


%% was: dom(A), true => tEQUALISH(A,A).
dom(A) => tEQUALISH(A,A).

tEQUALISH(A,B) => tEQUALISH(B,A).

tEQUALISH(A,B),
tREFLEXIVE_REWRITE(B,C) => tREFLEXIVE_REWRITE(A,C).

tEQUALISH(A,B) => tREFLEXIVE_REWRITE(A,B).

tREWRITE(A,B) => tREFLEXIVE_REWRITE(A,B).

tREFLEXIVE_REWRITE(A,B) => tEQUALISH(A,B); tREWRITE(A,B).

tREWRITE(A,B), tREWRITE(A,C) => dom(D), tREWRITE(B,D), tREWRITE(C,D). 
